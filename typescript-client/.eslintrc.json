{
  "root": true,  
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint", "prettier", "jest"],
  "extends": [
    "eslint:recommended",
    "plugin:prettier/recommended"
  ],
  "rules": {
    "no-console": 1,
    "prettier/prettier": 2
  },
  "env": {
    "browser": true,
    "node": true,
    "jest/globals": true
  }
}
